<service>
	<id>aiinitiativebackend.exe</id>
	<name>AI Initiative Backend</name>
	<description>AI Initiative Ideas Collection - Node.js Backend Server</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>--max_old_space_size=2048</argument>
	<argument>C:\Intellij_Workspace\ai-initiative-app\backend\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\Intellij_Workspace\ai-initiative-app\backend\server.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>AI Initiative Backend wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<env name="NODE_ENV" value="production"/>
	<env name="PORT" value="3000"/>
	<serviceaccount>
		<domain>HARSHA-PC</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>C:\Intellij_Workspace\ai-initiative-app\backend</workingdirectory>
</service>